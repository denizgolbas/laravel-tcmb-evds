{
    "name": "denizgolbas/laravel-tcmb-evds",
    "description": "TCMB EVDS2 API'si için Laravel paketi - Günlük döviz kurları",
    "type": "library",
    "keywords": [
        "laravel",
        "tcmb",
        "evds",
        "doviz",
        "exchange-rate",
        "tcmb-api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Deniz Gölbaş",
            "email": "denizgolbas@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^1.10 || ^2.0",
        "laravel/pint": "^1.0"
    },
    "conflict": {
        "phpunit/phpunit": "<10.0|>=12.0"
    },
    "autoload": {
        "psr-4": {
            "Denizgolbas\\LaravelTcmbEvds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Denizgolbas\\LaravelTcmbEvds\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denizgolbas\\LaravelTcmbEvds\\EvdsServiceProvider"
            ],
            "aliases": {
                "Evds": "Denizgolbas\\LaravelTcmbEvds\\Facades\\Evds"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "lint": "pint"
    }
}

